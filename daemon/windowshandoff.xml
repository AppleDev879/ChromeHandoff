<service>
	<id>windowshandoff.exe</id>
	<name>Windows Handoff</name>
	<description>Allows opening websites in Chrome that are open in Safari on an iPhone</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\Users\Andrew\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Users\Andrew\Documents\WindowsHandoff\express-server.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>Windows Handoff wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>DESKTOP-RE3VVCT</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\Users\Andrew\Documents\WindowsHandoff</workingdirectory>
</service>